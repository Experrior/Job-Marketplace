      "skill_name": "first_name",
      "proficiency_level": "first_name",
      "updated_at": "timestamp"
    },
    "chat_messages": {
      "message_id": "PK serial",
      "chat_id": "FK chats.chat_id",
      "content": "long_text",
      "created_by": "FK app_users.user_id",
      "read_by": "first_name",
      "deleted_by": "first_name",
      "created_at": "timestamp",
      "updated_at": "timestamp"
    },
    "job": {
      "job_id": "PK serial",
      "company_id": "FK company.company_id",
      "job_title": "first_name",
      "job_description": "long_text",
      "required_skills": "long_text",
      "required_experience": "long_text",
      "location": "first_name",
      "salary": "integer CONST None",
      "created_at": "timestamp"
    },
    "experiences": {
      "experience_id": "PK serial",
      "profile_id": "FK user_profiles.profile_id",
      "company_name": "FK company.company_id",
      "role": "first_name",
      "start_date": "timestamp",
      "end_date": "timestamp",
      "updated_at": "timestamp"
    },
    "educations": {
      "education_id": "PK serial",
      "profile_id": "first_name",
      "institution_name": "first_name",
      "degree": "first_name",
      "start_date": "timestamp",
      "end_date": "timestamp",
      "updated_at": "timestamp"
    },
    "company": {
      "company_id": "PK serial",
      "company_name": "first_name",
      "location": "first_name",
      "industry": "first_name",
      "description": "first_name",
      "verified": "bool",
      "created_at": "timestamp",
      "updated_at": "timestamp"
    },
    "chats": {
      "chat_id": "PK serial",
      "name": "first_name",
      "members": "first_name",
      "created_by": "first_name",
      "deleted_by": "first_name",
      "last_message": "first_name",
      "tags": "first_name",
      "created_at": "timestamp",
      "updated_at": "timestamp"
    },
    "user_profiles": {
      "profile_id": "PK serial",
      "user_id": "FK app_users.user_id",
      "resume_path": "first_name",
      "profile_picture_path": "first_name",
      "updated_at": "timestamp"
    },
    "user_settings": {
      "settings_id": "PK serial",
      "user_id": "FK app_users.user_id",
      "offers_notification": "bool",
      "newsletter_notification": "bool",
      "recruiter_messages": "bool",
      "push_notification": "bool",
      "updated_at": "timestamp"
    }
  },
