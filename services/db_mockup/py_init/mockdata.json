  "tables": {
    "app_users": {
      "user_id": "PK serial",
      "company": "FK company.company_id",
      "email": "email UNIQUE",
      "first_name": "first_name",
      "last_name": "last_name",
      "phone": "first_name",
      "role": "first_name",
      "is_blocked": "bool",
      "email_verified": "bool",
      "employee_verified": "bool",
      "created_at": "timestamp",
      "password_hash": "first_name"
